{
  "expo": {
    "name": "L'arganier",
    "slug": "larganier",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon2.png",
    "userInterfaceStyle": "light",
    "notification": {
      "icon": "./assets/icon2.png",
      "color": "#ffffff"
    },
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/icon2.png",
          "color": "#ffffff",
          "sounds": ["./assets/mixkit_magic_marimba_2820.wav"]
        }
      ]
    ],
    "splash": {
      "image": "./assets/splash2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch", "remote-notification"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive_icon2.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.imadmachi.larganier"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "7b5c5e04-3f55-48d8-bdc0-bb2b3ff309f0"
      }
    },
    "owner": "arganier"
  }
}
